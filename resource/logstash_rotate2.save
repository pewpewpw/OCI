/data/logstash/*.count{
	su conse conse
        daily
        dateext
        rotate 15
        copytruncate
        missingok
        notifempty
        olddir /data/logstash/old_log
}


/data/logstash/debug.txt{
	su conse conse
        daily
        dateext
        rotate 7
        copytruncate
        compress
        delaycompress
        missingok
        notifempty
        olddir /data/logstash/old_log
}


